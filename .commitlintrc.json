{
  "extends": ["@commitlint/config-conventional"],
  "rules": {
    "type-enum": [
      2,
      "always",
      ["build", "chore", "docs", "feat", "fix", "perf", "refactor", "revert", "style", "test"]
    ],
    "scope-enum": [
      2,
      "always",
      [
        "root",
        "ci",
        "wiki",
        "release",
        "test-client",
        "http",
        "fetch",
        "interceptor",
        "utils",
        "eslint",
        "eslint-node",
        "eslint-react",
        "lint-staged",
        "tsconfig",
        "examples",
        "example-interceptor-vitest-node",
        "example-interceptor-vitest-browser",
        "example-interceptor-vitest-jsdom",
        "example-interceptor-jest-node",
        "example-interceptor-jest-jsdom",
        "example-interceptor-playwright",
        "example-interceptor-next-js-app",
        "example-interceptor-next-js-pages",
        "example-interceptor-openapi-typegen"
      ]
    ],
    "scope-empty": [2, "never"]
  }
}
