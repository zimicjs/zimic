{
  "semi": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "proseWrap": "always",
  "plugins": ["prettier-plugin-sh", "prettier-plugin-jsdoc", "prettier-plugin-tailwindcss"]
}
