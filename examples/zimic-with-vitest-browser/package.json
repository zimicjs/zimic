{
  "name": "zimic-example-with-vitest-browser",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "test": "vitest",
    "test:turbo": "dotenv -v CI=true -- pnpm run test run",
    "types:check": "tsc --noEmit",
    "deps:setup-playwright": "playwright install chromium",
    "deps:setup-zimic": "zimic-interceptor browser init ./public || true",
    "deps:setup": "concurrently --names playwright,zimic 'pnpm deps:setup-playwright' 'pnpm deps:setup-zimic'"
  },
  "dependencies": {
    "@zimic/fetch": "latest",
    "@zimic/http": "latest"
  },
  "devDependencies": {
    "@vitest/browser": "^3.2.4",
    "@zimic/interceptor": "latest",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^10.0.0",
    "playwright": "^1.56.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
